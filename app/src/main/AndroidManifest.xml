<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.app.tfmuoc001">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/appName"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Main">
        <activity android:name=".MainPortScannerActivity" />
        <activity android:name=".MainNetworkScannerActivity" />
        <activity
            android:name=".PortScannerPreferencesActivity"
            android:theme="@style/Theme.Preferences" />
        <activity
            android:name=".NetworkScannerPreferencesActivity"
            android:theme="@style/Theme.Preferences" />
        <activity
            android:name=".PortScannerActivity"
            android:theme="@style/Theme.Scanner" />
        <activity
            android:name=".NetworkScannerActivity"
            android:theme="@style/Theme.Scanner" />
        <activity
            android:name=".SplashScreenActivity"
            android:theme="@style/Theme.Splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainPagePreferencesActivity"
            android:theme="@style/Theme.Preferences" />
        <activity android:name=".MainPageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>